public class APArrayIntList implements APIntList
{ 
    
    int[] array = new int [1000];
    private int size;
    public int size(){
        return size;
    }
    
    public void add(int item){
        array[size]=item;
        size+=1;
    }
    public int get(int index){
        return array[index];
    }
    
}
